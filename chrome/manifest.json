{
  "manifest_version": 3,
  "name": "BirdTab",
  "version": "1.0",
  "description": "Transform your new tabs into a vibrant birding experience. Discover stunning birds daily with high-quality images and fascinating facts.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://birdtab-proxy.vercel.app/*"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "19": "icons/icon19.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}